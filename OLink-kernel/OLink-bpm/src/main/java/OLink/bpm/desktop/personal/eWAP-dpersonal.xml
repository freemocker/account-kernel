				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
		<action name="save"
			class="OLink.bpm.desktop.personal.DPersonalAction"
			method="doSave">
			<result name="success" type="dispatcher">
				/desktop/xml.jsp
			</result>
			<result name="error" type="dispatcher">
				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
		<action name="delete"
			class="OLink.bpm.desktop.personal.DPersonalAction"
			method="doDelete">
			<result name="success" type="dispatcher">
				/desktop/xml.jsp
			</result>
			<result name="error" type="dispatcher">
				../error.jsp
			</result>
			<result name="input" type="dispatcher">
				../error.jsp
			</result>
		</action>
	</package>

	
</xwork>