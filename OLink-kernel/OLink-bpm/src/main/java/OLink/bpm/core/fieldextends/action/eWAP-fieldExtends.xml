<xwork>
	<package name="ufieldextends" extends="eWAP-default"
		namespace="/core/fieldextends">
		<action name="new"
				class="OLink.bpm.core.fieldextends.action.FieldExtendsAction" method="doNew">
			<result name="input" type="dispatcher">content.jsp</result>
			<result name="success" type="dispatcher">content.jsp</result>
			
			<interceptor-ref name="params" />
		</action>
		<action name="view"
				class="OLink.bpm.core.fieldextends.action.FieldExtendsAction" method="doView">
