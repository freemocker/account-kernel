<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
<package name="eWAP-helper" extends="eWAP-default"
		namespace="/core/helper">
		<action name="new" class="OLink.bpm.core.helper.action.HelperAction"
			method="doNew">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		<action name="edit"
				class="OLink.bpm.core.helper.action.HelperAction" method="doEdit">
			<result name="success" type="dispatcher">
				content.jsp
			</result>
			<interceptor-ref name="params" />
		</action>
		<action name="save"
				class="OLink.bpm.core.helper.action.HelperAction" method="doSave">
			<result name="input" type="dispatcher">
			content.jsp
			</result>
			<result name="success" type="dispatcher">
			content.jsp
			</result>
			<interceptor-ref name="validationWorkflowStack"/>
					</action>
		<action name="delete"
				class="OLink.bpm.core.helper.action.HelperAction" method="doDelete">
			<interceptor-ref name="params" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="chain">list</result>
			<result name="input" type="chain">list</result>
		</action>
		<action name="list"
				class="OLink.bpm.core.helper.action.HelperAction" method="doList">
			<interceptor-ref name="chain" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="dispatcher">list.jsp</result>
			<result name="input" type="dispatcher">list.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
		<action name="newOrEdit"
				class="OLink.bpm.core.helper.action.HelperAction" method="doNewOrEdit">
			<interceptor-ref name="chain" />
			<interceptor-ref name="basicStack" />
			<result name="success" type="dispatcher">list.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="prepare" />
		</action>
		
		<action name="getHelpTreeIndex" class="OLink.bpm.core.helper.action.HelperAction" method="doHelpTreeIndex">
			<result name="success" type="dispatcher">/admin/main.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		
		<action name="getTipicHrefById" class="OLink.bpm.core.helper.action.HelperAction" method="getTipicHrefById">
			<result name="success" type="dispatcher">/admin/main.jsp</result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		
		<action name="getIscriptHelp" class="OLink.bpm.core.helper.action.HelperAction" method="getIscriptHelp">
			<result name="success" type="dispatcher"></result>
			<interceptor-ref name="params" />
			<interceptor-ref name="chain" />
		</action>
		
	</package>
	
</xwork>